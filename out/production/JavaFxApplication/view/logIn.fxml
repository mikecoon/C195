<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginController">
    <children>
        <TextField fx:id="loginUsername" layoutX="218.0" layoutY="167.0" />
        <TextField fx:id="loginPassword" layoutX="218.0" layoutY="215.0" />
        <Label fx:id="loginLabel" alignment="TOP_CENTER" layoutX="94.0" layoutY="47.0" prefHeight="88.0" prefWidth="402.0" text="Login" textAlignment="CENTER">
            <font>
                <Font size="60.0" />
            </font>
        </Label>
        <Button fx:id="loginLoginButton" layoutX="223.0" layoutY="262.0" mnemonicParsing="false" onAction="#loginButton" prefHeight="31.0" prefWidth="72.0" text="Login" />
        <Button fx:id="loginExitButton" layoutX="303.0" layoutY="262.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="31.0" prefWidth="72.0" text="Exit" />
        <TextField fx:id="loginLocation" layoutX="218.0" layoutY="315.0" />
        <Label fx:id="loginLocationLabel" layoutX="118.0" layoutY="319.0" text="Location" />
      <Label fx:id="loginUsernameLabel" layoutX="114.0" layoutY="171.0" text="Username" />
      <Label fx:id="loginPasswordLabel" layoutX="116.0" layoutY="219.0" text="Password" />
    </children>
</AnchorPane>
